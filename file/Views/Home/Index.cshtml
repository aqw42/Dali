<img class="bottle2" src="https://www.castillon-cotesdebordeaux.com/wp-content/uploads/2016/03/Demoiselles.png" />
<img class="bottle3" height="150" src="https://www.castillon-cotesdebordeaux.com/wp-content/uploads/2016/03/Demoiselles.png" />

<script type="text/javascript" src="~/Scripts/jquery-3.4.1.min.js"></script>
<script type="text/javascript" src="~/Scripts/Dali.js"></script>


<link rel="stylesheet" type="text/css" src="~/Content/Site.css">
<style>
    body {
        background: radial-gradient(hsl(0, 100%, 27%) 4%, hsl(0, 100%, 18%) 9%, hsla(0, 100%, 20%, 0) 9%) 0 0, radial-gradient(hsl(0, 100%, 27%) 4%, hsl(0, 100%, 18%) 8%, hsla(0, 100%, 20%, 0) 10%) 50px 50px, radial-gradient(hsla(0, 100%, 30%, 0.8) 20%, hsla(0, 100%, 20%, 0)) 50px 0, radial-gradient(hsla(0, 100%, 30%, 0.8) 20%, hsla(0, 100%, 20%, 0)) 0 50px, radial-gradient(hsla(0, 100%, 20%, 1) 35%, hsla(0, 100%, 20%, 0) 60%) 50px 0, radial-gradient(hsla(0, 100%, 20%, 1) 35%, hsla(0, 100%, 20%, 0) 60%) 100px 50px, radial-gradient(hsla(0, 100%, 15%, 0.7), hsla(0, 100%, 20%, 0)) 0 0, radial-gradient(hsla(0, 100%, 15%, 0.7), hsla(0, 100%, 20%, 0)) 50px 50px, linear-gradient(45deg, hsla(0, 100%, 20%, 0) 49%, hsla(0, 100%, 0%, 1) 50%, hsla(0, 100%, 20%, 0) 70%) 0 0, linear-gradient(-45deg, hsla(0, 100%, 20%, 0) 49%, hsla(0, 100%, 0%, 1) 50%, hsla(0, 100%, 20%, 0) 70%) 0 0;
        background-color: #300;
        background-size: 100px 100px;
    }

    .tablo {
        cursor: url("data:image/png;base64,") 0 75, auto;
        position: relative;
        margin: auto;
        left: 0px;
        right: 0px;
        margin-top: 10%;
        width: 460px;
        height: 460px;
        animation: canvas-anim 4s ease-in-out infinite;
        transform-origin: 50% 0%
    }

    .canvas, .background, .draw {
        position: absolute;
        margin: auto;
        top: 0px;
        bottom: 0px;
        left: 0px;
        right: 0px;
    }

    .draw {
        z-index: 100;
    }

    @@keyframes canvas-anim {
        0% {
            transform: rotate(5deg);
        }

        50% {
            transform: rotate(-5deg);
        }

        100% {
            transform: rotate(5deg);
        }
    }

    .bottle {
        position: fixed;
        bottom: -20px;
        left: 20px;
        width: 70px;
        height: 150px;
    }

    .bottle2 {
        position: fixed;
        bottom: -20px;
        left: 40px;
        width: 90px;
        height: 190px;
        transform: rotate(0deg);
    }
    .bottle2:hover {
        animation: fall 1s cubic-bezier(1,.02,.62,1.05);
        transform-origin: bottom;
        animation-fill-mode: forwards;
    }

    @@Keyframes fall {
        0% { 
            transform: rotate(0deg);
        }
        100% {
            transform: rotate(-25deg);
        }
    } 

    .bottle3 {
        position: fixed;
        bottom: -60px;
        left: 30px;
        width: 70px;
        height: 150px;
        transform: rotate(90deg);
    }
</style>

